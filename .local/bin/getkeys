#!/bin/sh
## 2025-01-09 SARBS
# ist dafür verantwortlich die eigenen Hilfdokumente anzuzeigen

cat "${XDG_DATA_HOME:-$HOME/.local/share}"/sarbs/getkeys/"$1" 2>/dev/null && exit
echo "Führe den Befehl mit einem der folgenden Argumente aus, um Informationen über dieses Programm zu erhalten:"
ls "${XDG_DATA_HOME:-$HOME/.local/share}"/sarbs/getkeys
