#!/bin/bash
## 2025-01-09 SARBS
# Startskript für Telegram das Theme wird automatisch aktualisiert..
# Einrichtung:
# walogram-git instalieren
# in Telegramm das Theme ausgewählen:
# ~/.cache/walogram/wal.tdesktop-theme

BG_PATH=~/.local/share/bg
THEME_PATH=~/.cache/walogram/wal.tdesktop-theme

walogram -i "$BG_PATH" >/dev/null 2>&1
# walogram -B -i "$BG_PATH" >/dev/null 2>&1

# Startet Telegram
Telegram &
